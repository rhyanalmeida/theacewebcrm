# Supabase Configuration
# Copy this to .env.local and fill in your actual values

# Supabase Project Settings
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database Settings
SUPABASE_DB_URL=postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres
SUPABASE_JWT_SECRET=your-jwt-secret

# Storage Settings
SUPABASE_STORAGE_BUCKET=ace-crm-files

# Edge Functions
SUPABASE_EDGE_FUNCTIONS_URL=https://your-project-ref.functions.supabase.co

# Environment
NODE_ENV=development
SUPABASE_DEBUG=true

# ACE CRM Specific
CRM_DEFAULT_ROLE=user
CRM_ADMIN_EMAIL=admin@acewebdesigners.com
CRM_COMPANY_NAME=ACE Web Designers
CRM_COMPANY_DOMAIN=acewebdesigners.com